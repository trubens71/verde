{
  "$schema": "https://smcse.city.ac.uk/student/aczd178/verde/schema/verde-meta-schema#",
  "$id": "https://smcse.city.ac.uk/student/aczd178/verde/schema/asc-domain-schema#",
  "title": "A simple domain schema",
  "description": "A simple schema to be validated with a meta-schema",
  "$comment": "Version 1: ",
  "definitions": {},
  "type": "object",
  "properties": {
    "user": {
      "type": "object",
      "properties": {
        "quality_of_life": {
          "type": "object",
          "domain_meta": {
            "phase": "outcome",
            "explains": [
              {
                "funder": {
                  "org_unit": {
                    "region_code": true
                  }
                }
              }
            ]
          },
          "properties": {
            "well-being": {
              "type": "boolean",
              "domain_meta": {
                "explains": [
                  {
                    "funder": {}
                  }
                ],
                "phase": "outcome",
                "ordinal": ["bad", "average", "good"]
              }
            },
            "independence-mobility": {
              "type": "boolean",
              "domain_meta": {
                "phase": "outcome"
              }
            }
          }
        }
      }
    }
  }
}
